<UserControl x:Class="Count4U.ExportErpTextAdapter.RetalixPOS_HO.ExportErpRetalixPOS_HOAdapterView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Localization="clr-namespace:Count4U.Localization;assembly=Count4U.Localization" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <RadioButton Content="{x:Static Localization:Resources.View_ExportErp_radioMakat}"
                     Height="16" HorizontalAlignment="Left" Margin="12,12,0,0" Name="checkBox1" VerticalAlignment="Top" Width="120" 
                  IsChecked="{Binding Path=Makat, UpdateSourceTrigger=PropertyChanged}"/>

        <RadioButton Content="{x:Static Localization:Resources.View_ExportErp_radioMakatOriginal}"
                     Height="17" HorizontalAlignment="Left" Margin="12,34,0,0" Name="checkBox2" VerticalAlignment="Top" Width="99"
                  IsChecked="{Binding Path=MakatOriginal, UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</UserControl>
